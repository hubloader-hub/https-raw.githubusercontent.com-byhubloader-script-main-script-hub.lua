-- // Services
local Players = game:GetService("Players")
local RunService = game:FindService("RunService")
local VirtualUser = game:GetService("VirtualUser")
-- // Variables
local Player = Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:Wait()
local HRP:Part = Char:WaitForChild("HumanoidRootPart")
local Humanoid = Char:FindFirstChildOfClass("Humanoid")
local AntiAFK = false
local ControlPoint:Model = workspace.Control_Point
local ControlPointPart:Part = ControlPoint.Point
local AutoFarm = {
	
	StealDollars = false,
	AutoCollect = false,
	AutoBuild = false,
	AutoRebith = false, -- This not Used Yet, I can't make it work :(
	AutoHillRemoval = false
	
}
local Trolling = {
	
	Fling = false,
	TargetPlayer = nil,
	Targeting = false
	
}
local Combat = {
	
	ESP = false,
	EquipAll = false
	
}
local Tycoon = workspace.Tycoons:FindFirstChild(Player.Name)
if not Tycoon then
	Rayfield:Notify({
		Title = "Error",
		Content = "Collect Your Tycoon!",
		Duration = 6.5,
		Image = 11745872910,
		Actions = { -- Notification Buttons
			Ignore = {
				Name = "Okay",
				Callback = function()
					print("k!")
				end
			},
		},
	})
end
repeat
	task.wait(1) -- To prevent roblox from freezing :0
	Tycoon = workspace.Tycoons:FindFirstChild(Player.Name)
until Tycoon
local PlayerBalance:IntValue = Player.leaderstats.Money

-- // Scripts

local OuxiHubOP = "https://pastebin.com/raw/Qt6Qni6d"

-- // Functions

Player.CharacterAdded:Connect(function(NewChar)
	Char = NewChar
	HRP = Char:WaitForChild("HumanoidRootPart")
	Humanoid = Char:FindFirstChildOfClass("Humanoid")
end)

local function GetPlayerFromDisplayOrUserName(PlayerName:string)
	local finded = false
	for _, p in pairs(Players:GetPlayers()) do
		if p.Name:lower() == PlayerName:lower() or p.DisplayName:lower() == PlayerName:lower() then
			finded = true
			return p
		end
	end
	if not finded then
		return nil
	end
end

local function FlingRequest()
	local Target = Trolling.TargetPlayer
	local Thrust = Instance.new('BodyThrust', HRP)
	Thrust.Force = Vector3.new(9999,9999,9999)
	Thrust.Name = "YeetForce"

	if Trolling.Fling then
		repeat
			HRP.CFrame = Target.Character.HumanoidRootPart.CFrame
			Thrust.Location = Target.Character.HumanoidRootPart.Position
			RunService.Heartbeat:wait()
		until not Trolling.Fling
	else
		Thrust:Destroy()
	end
end

local function TargetingRequest()
	local FollowWeld = Instance.new("WeldConstraint", Char.HumanoidRootPart)
	if Trolling.TargetPlayer then
		HRP.CFrame = Trolling.TargetPlayer.Character.HumanoidRootPart.CFrame
		HRP.CFrame = CFrame.new(HRP.Position + Vector3.new(0,5,0), Vector3.new(0,-90,0))
		FollowWeld.Part0 = Trolling.TargetPlayer.Character.HumanoidRootPart
		FollowWeld.Part1 = HRP
		Player.CharacterAdded:Connect(function(Char)
			FollowWeld = Instance.new("WeldConstraint", HRP)
			Char.HumanoidRootPart.CFrame = Trolling.TargetPlayer.Character.HumanoidRootPart.CFrame
			Char.HumanoidRootPart.CFrame = CFrame.new(Char.HumanoidRootPart.Position + Vector3.new(0,5,0), Vector3.new(0,-90,0))
			FollowWeld.Part0 = Trolling.TargetPlayer.Character.HumanoidRootPart
			FollowWeld.Part1 = Char.HumanoidRootPart
		end)
	else
		FollowWeld:Destroy()
	end
end

task.spawn(function()
	while task.wait(1) do
		if AutoFarm.AutoCollect then
			Char:PivotTo(Tycoon.Auxiliary.Collector.Collect.CFrame)
		end
		
		if AutoFarm.AutoHillRemoval and ControlPoint:GetAttribute("Owner") ~= Player.Name then
			local StartCFrame = HRP.CFrame
			repeat
				task.wait() -- To prevent roblox from freezing :0
				Char:PivotTo(ControlPointPart.CFrame)
			until ControlPoint:GetAttribute("Owner") == Player.Name
			Char:PivotTo(StartCFrame)
		end
		
		if AutoFarm.AutoBuild then
			for _, b in pairs(Tycoon.Buttons:GetChildren()) do
				local ButtonBase:Part = b.Button
				
				if ButtonBase.Color == Color3.fromRGB(0,127,0) then
					Char:PivotTo(ButtonBase.CFrame)
				end
			end
		end
	end
end)

workspace.ChildAdded:Connect(function(O)
	if O.Name == "Dollar" and AutoFarm.StealDollars then
		for i, o2 in pairs(workspace:GetChildren()) do
			if o2.Name == "Dollar" then
				task.wait()
				local StartCFrame = HRP.CFrame
					Char:PivotTo(o2.CFrame)
					task.wait()
					Char:PivotTo(StartCFrame)
			end
		end
	end
end)

Player.Idled:Connect(function()
	if AntiAFK then
		VirtualUser:ClickButton2(Vector2.new())
	end
end)

