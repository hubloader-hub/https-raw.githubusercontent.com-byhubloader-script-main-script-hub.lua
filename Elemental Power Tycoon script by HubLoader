local Window = Rayfield:CreateWindow({
	Name = "Tycoon nguyên tố sức mạnh by HubLoader",
	LoadingTitle = "Rayfield Interface Suite",
	LoadingSubtitle = "by HubLoader",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "Star Yield", -- Create a custom folder for your hub/game
		FileName = "StarYield_Elemental Powers Tycoon__config"
	},
	Discord = {
		Enabled = true,
		Invite = "ZC2", -- The Discord invite code, do not include discord.gg/. E.g. di.gg would be ABCD
		RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Untitled",
		Subtitle = "Key System",
		Note = "No method of obtaining the key is provided",
		FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
		SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = {""} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
	}
})

local Tab = Window:CreateTab("cày", 107364918194164)
local Tabe = Window:CreateTab("chiến đấu", 80964454894155)
local Tab2 = Window:CreateTab("Troll việt nam", 108044792581952)

local Button = Tab:CreateButton({
	Name = "trở về nhà",
	Callback = function()
		local Tycoon = workspace:WaitForChild("Tycoons"):FindFirstChild(Player.Name)
		if Tycoon then
			Char:PivotTo(Tycoon.Auxiliary.Spawn:FindFirstChildOfClass("SpawnLocation").CFrame)
		end
	end,
})  

